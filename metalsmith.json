{
  "clean": false,
  "source": "pages",
  "destination": "dist",
  "plugins": [
    {
      "metalsmith-fs-metadata": {
        "config": "./config/site.json"
      }
    },
    {
      "metalsmith-path": {
        "directoryIndex": "/index.html",
        "extensions": [
          ".html"
        ]
      }
    },
    {
      "lib/metalsmith-build-info.js": true
    },
    {
      "lib/metalsmith-env-info.js": true
    },
    {
      "metalsmith-frontmatter-file-loader": {
        "key": "blocks-md",
        "suppressNoFilesError": true
      }
    },
    {
      "metalsmith-frontmatter-renderer": {
        "key": "blocks-md",
        "out": "blocks",
        "suppressNoFilesError": true
      }
    },
    {
      "metalsmith-frontmatter-file-loader": {
        "key": "blocks-njk",
        "suppressNoFilesError": true
      }
    },
    {
      "metalsmith-frontmatter-renderer": {
        "key": "blocks-njk",
        "out": "blocks",
        "ext": "njk",
        "suppressNoFilesError": true
      }
    },
    {
      "metalsmith-layouts": true
    },
    {
      "metalsmith-beautify": {
        "preserve_newlines": false
      }
    },
    {
      "metalsmith-drafts": true
    },
    {
      "lib/metalsmith-html-minifier-optimise.js": {
        "minifierOptions": {
          "removeComments": false
        }
      }
    },
    {
      "lib/metalsmith-mapsite-currentenv.js": {
        "omitIndex": true
      }
    }
  ]
}
